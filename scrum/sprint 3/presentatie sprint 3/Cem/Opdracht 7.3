using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Opdracht7._3
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] getallen = new int[25];
            Random r = new Random();
            r.Next(1, 100000);
            for (int i = 0; i < 25 ; i++)
            {
                getallen[i] = r.Next(1, 10000);
            }


            for (int i = 0; i < 25; i++)
            {
                if (getallen[i] % 2 == 0)
                {
                    Console.WriteLine(getallen[i] + ": even");
                }
                else 
                {
                    Console.WriteLine(getallen[i] + ": oneven");
                }
            }
            Console.ReadKey();
        }
    }	
}
